var missingNumber = function (nums) {
    let length = (nums.length + 1);
    sumTotal = 0;
    missing = 0;
    //nums.sort()
    for (let i = 0; i < length; i++) {
        //console.log(i)
        sumTotal += i
        if (nums[i]) {
            //console.log(nums[i])
            missing += nums[i]
        }
    }
    return sumTotal - missing

};
// sort method
// hasmap - linear time - O(n) for time and space
// gSum = n*(n+1)/2 = 6 - constant time 0(1)


